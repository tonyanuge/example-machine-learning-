{
"manifest_version": 3,
    "name": "Hate Less",
    "description": "This extension censors offensive language and removes hateful tweets.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
